{
  "name": "@google-cloud/recaptcha-cloudflare",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "dist/cloudflare_worker.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/src/index.d.ts"
    }
  },
  "scripts": {
    "build": "npx tsc && esbuild --bundle src/index.ts --platform=node --format=esm --outdir=dist",
    "buildworker": "wrangler deploy --dry-run --outdir=dist",
    "deploy": "wrangler deploy src/cloudflare_worker.ts",
    "dev": "npx wrangler dev",
    "start": "npx wrangler dev",
    "test": "vitest run",
    "devclean": "rm -rf dist && rm package-lock.json && rm -rf node_modules"
  },
  "dependencies": {
    "@google-cloud/recaptcha": "^1.0.0",
    "@worker-tools/html-rewriter": "^0.1.0-pre.19"
  },
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.4.12",
    "@cloudflare/workers-types": "^4.20231016.0",
    "miniflare": "^3.20240701.0",
    "typescript": "^5.0.4",
    "vitest": "1.5.0",
    "vitest-environment-miniflare": "^2.14.2",
    "wrangler": "^3.60.0"
  },
  "workspaces": [
    "../../"
  ]
}
