{
  "name": "@google-cloud/recaptcha",
  "version": "1.0.0",
  "description": "recaptcha enterprise typescript library",
  "main": "dist/index.js",
  "exports": "./dist/index.esm.js",
  "type": "module",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "prepublishOnly": "npm run build",
    "test": "vitest",
    "devbuild": "npm install && npm run genproto && npm run build && mkdir -p dist/generated && cp -r src/generated/ dist/generated/",
    "devclean": "rm -rf dist && rm -rf node_modules && rm -rf coverage && rm package-lock.json",
    "coverage": "vitest run --coverage",
    "genproto": "mkdir -p src/generated && npx protoc --ts_out src/generated --proto_path src/proto src/proto/soz.proto src/proto/status.proto src/proto/recaptchaenterprise.proto",
    "genclean": "rm -rf src/generated"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.26.0",
    "@protobuf-ts/plugin": "^2.9.4",
    "ipaddr.js": "^2.2.0",
    "picomatch": "^4.0.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.49.0",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^20.14.10",
    "@types/picomatch": "^2.3.4",
    "@vitest/coverage-v8": "^1.6.0",
    "cross-fetch": "^4.0.0",
    "ts-protoc-gen": "^0.15.0",
    "ts-standard": "^12.0.2",
    "tslib": "^2.7.0",
    "typescript": "^5.4.5",
    "vitest": "1.6.0"
  }
}
